<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>se.uu.ub.cora</groupId>
		<artifactId>cora-parent</artifactId>
		<version>2.3.0</version>
	</parent>

	<artifactId>coradata-spies</artifactId>
	<version>1.3.0-SNAPSHOT</version>
	<packaging>jar</packaging>

	<name>Coradata-spies</name>

	<properties>
		<coradata.version>7.5.0</coradata.version>
		<json.version>3.7.0</json.version>
		<testutils.version>1.9.0</testutils.version>
		<testutils-spies.version>1.3.0</testutils-spies.version>
	</properties>

	<scm>
		<connection>scm:git:https://github.com/lsu-ub-uu/cora-data-spies.git</connection>
		<developerConnection>scm:git:https://github.com/lsu-ub-uu/cora-data-spies.git</developerConnection>
